{
  "extends": [
    "eslint-config-carnesen"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "all",
        "bracketSpacing": true,
        "useTabs": true,
        "tabWidth": 2,
        "printWidth": 60
      }
    ]
  }
}
