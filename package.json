{
  "name": "@carnesen/cli",
  "description": "A library for building Node.js command-line interfaces (CLIs)",
  "version": "0.3.3",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "rebuild": "rimraf lib && npm run build",
    "cli": "ts-node src/examples/index.ts",
    "lint": "tslint --project tsconfig.build.json",
    "fix": "npm run lint -- --fix",
    "test": "npm run lint && npm run unit-test && npm run rebuild && npm run cli -- echo ok",
    "unit-test": "jest src"
  },
  "dependencies": {
    "@types/parse-json": "4.0.0",
    "parse-json": "5.0.0",
    "redent": "3.0.0"
  },
  "devDependencies": {
    "@carnesen/coded-error": "0.1.1",
    "@carnesen/run-and-catch": "0.1.1",
    "@carnesen/tsconfig": "0.2.0",
    "@carnesen/tslint-config": "0.2.0",
    "@types/jest": "25.2.3",
    "@types/node": "14.0.4",
    "jest": "26.0.1",
    "np": "6.2.3",
    "rimraf": "3.0.2",
    "ts-jest": "26.0.0",
    "ts-node": "8.10.1",
    "tslint": "6.1.2",
    "typescript": "3.9.3"
  },
  "files": [
    "src",
    "lib"
  ],
  "keywords": [
    "typescript",
    "nodejs",
    "cli",
    "command-line-interface"
  ],
  "author": {
    "name": "Chris Arnesen",
    "email": "chris.arnesen@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/carnesen/cli.git"
  },
  "bugs": {
    "url": "https://github.com/carnesen/cli/issues"
  },
  "homepage": "https://github.com/carnesen/cli#readme",
  "license": "MIT"
}
