{
	"name": "@carnesen/cli-website-server",
	"description": "Node.js server for cli.carnesen.com",
	"version": "2020.6.3",
	"scripts": {
		"build": "rimraf lib/ && tsc",
		"lint": "eslint --ext .ts,.tsx src",
		"start": "node lib/server.js",
		"watch": "nodemon --exec ts-node --ext ts src/server.ts",
		"test": "npm run lint && npm run unit-test && npm run build",
		"unit-test": "jest src",
		"prepublishOnly": "npm run test"
	},
	"dependencies": {
		"@carnesen/cli-website": "2020.6.3",
		"http-status-codes": "1.4.0",
		"koa": "2.13.0",
		"koa-static": "5.0.0"
	},
	"devDependencies": {
		"@carnesen/tsconfig": "0.3.1",
		"@types/jest": "26.0.0",
		"@types/koa": "2.11.3",
		"@types/koa-static": "4.0.1",
		"@types/node": "14.0.13",
		"eslint": "7.2.0",
		"eslint-config-carnesen": "4.0.0",
		"jest": "26.0.1",
		"nodemon": "2.0.4",
		"rimraf": "3.0.2",
		"ts-jest": "26.1.0",
		"ts-node": "8.10.2",
		"typescript": "3.9.5"
	},
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"src",
		"lib",
		"package-lock.json"
	],
	"repository": "github:carnesen/cli",
	"author": "Chris Arnesen",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/carnesen/cli/issues"
	},
	"homepage": "https://github.com/carnesen/cli/website-server#readme"
}
