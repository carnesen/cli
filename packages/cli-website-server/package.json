{
	"name": "@carnesen/cli-website-server",
	"description": "Node.js server for cli.carnesen.com",
	"version": "2022.5.0",
	"engines": {
		"node": ">=14.0.0"
	},
	"scripts": {
		"build": "./scripts/build.sh",
		"lint": "eslint --ext .js,.jsx,.ts,.tsx src jest.config.js",
		"lint:fix": "npm run lint -- --fix",
		"start": "node lib/server.js",
		"start:watch": "nodemon --exec ts-node --ext ts src/server.ts",
		"release": "carnesen-dev release --semverBump date",
		"test": "npm run lint && npm run unit-test && npm run build && npm pack",
		"unit-test": "jest src"
	},
	"dependencies": {
		"@carnesen/cli-docs": "0.7.1",
		"@carnesen/cli-website": "2022.5.0",
		"@types/koa-mount": "4.0.1",
		"http-status-codes": "2.2.0",
		"koa": "2.13.4",
		"koa-mount": "4.0.0",
		"koa-static": "5.0.0"
	},
	"devDependencies": {
		"@carnesen/dev": "0.1.1",
		"@carnesen/tsconfig": "0.4.1",
		"@types/jest": "27.4.1",
		"@types/koa": "2.13.4",
		"@types/koa-static": "4.0.2",
		"@types/node": "17.0.21",
		"eslint": "7.32.0",
		"eslint-config-carnesen": "7.0.0",
		"jest": "27.5.1",
		"nodemon": "2.0.15",
		"ts-jest": "27.1.3",
		"ts-node": "10.5.0",
		"typescript": "4.5.5"
	},
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist",
		"lib",
		"public",
		"src",
		"package-lock.json"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/carnesen/cli.git",
		"directory": "website-server"
	},
	"author": "Chris Arnesen",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/carnesen/cli/issues"
	},
	"homepage": "https://github.com/carnesen/cli/website-server#readme"
}
