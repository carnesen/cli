{
	"name": "@carnesen/cli-website-server",
	"description": "Node.js server for cli.carnesen.com",
	"version": "2021.10.0",
	"engines": {
		"node": ">=12.0.0"
	},
	"scripts": {
		"build": "./build.sh",
		"lint": "eslint --ext .js,.jsx,.ts,.tsx src jest.config.js",
		"fix": "npm run lint -- --fix",
		"start": "node lib/server.js",
		"start-watch": "nodemon --exec ts-node --ext ts src/server.ts",
		"test": "npm run lint && npm run unit-test && npm run build && npm pack",
		"unit-test": "jest src",
		"prepublishOnly": "npm run test"
	},
	"dependencies": {
		"@carnesen/cli-docs": "0.5.2",
		"@carnesen/cli-website": "2021.10.0",
		"@types/koa-mount": "4.0.0",
		"http-status-codes": "1.4.0",
		"koa": "2.13.0",
		"koa-mount": "4.0.0",
		"koa-static": "5.0.0"
	},
	"devDependencies": {
		"@carnesen/tsconfig": "0.4.0",
		"@types/jest": "26.0.8",
		"@types/koa": "2.11.3",
		"@types/koa-static": "4.0.1",
		"@types/node": "14.0.27",
		"eslint": "7.32.0",
		"eslint-config-carnesen": "7.0.0",
		"jest": "26.2.2",
		"nodemon": "2.0.4",
		"ts-jest": "26.1.4",
		"ts-node": "8.10.2",
		"typescript": "3.9.7"
	},
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist",
		"lib",
		"public",
		"src",
		"package-lock.json"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/carnesen/cli.git",
		"directory": "website-server"
	},
	"author": "Chris Arnesen",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/carnesen/cli/issues"
	},
	"homepage": "https://github.com/carnesen/cli/website-server#readme"
}
